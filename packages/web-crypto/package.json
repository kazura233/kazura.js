  "description": "web-crypto",
  "keywords": [
    "crypto",
    "rsa",
    "aes",
    "md5"
  ],
  "homepage": "https://github.com/kazura233/kazurajs/tree/master/packages/web-crypto#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazura233/kazurajs.git"
  },
  "bugs": "https://github.com/kazura233/kazurajs/issues",
  "author": {
    "name": "kazura233",
    "email": "kazira233@gmail.com"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./es/index.js",
      "types": "./types/index.d.ts"
    },
    "./aes-128-ecb": {
      "require": "./lib/aes-128-ecb.js",
      "import": "./es/aes-128-ecb.js",
      "types": "./types/aes-128-ecb.d.ts"
    },
    "./base64": {
      "require": "./lib/base64.js",
      "import": "./es/base64.js",
      "types": "./types/base64.d.ts"
    },
    "./md5": {
      "require": "./lib/md5.js",
      "import": "./es/md5.js",
      "types": "./types/md5.d.ts"
    },
    "./rsa-1024": {
      "require": "./lib/rsa-1024.js",
      "import": "./es/rsa-1024.js",
      "types": "./types/rsa-1024.d.ts"
    }
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "types",
    "iife"
  ],
  "scripts": {
    "clean": "rimraf dist lib es types iife",
    "dev": "rollup --config rollup.config.js --watch",
    "build": "pnpm clean & rollup --config rollup.config.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.11",
    "@types/crypto-js": "^4.1.1",
    "crypto-js": "^4.1.1",
    "jsencrypt": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.14",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.1",
    "rollup-plugin-typescript2": "^0.35.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "npmName": "@kazura233/web-crypto",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
